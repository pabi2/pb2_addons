	<subDataset name="po_line">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
pol.name,
pol.price_unit,
CONCAT(pol.product_qty::integer,' ',pu.name) product_unit,
pol.product_qty,
pu.name product_uom
FROM purchase_order_line pol
LEFT JOIN product_uom pu
ON pu.id = pol.product_uom
WHERE pol.order_id = $P{ids} and pol.active = True]]>
		</queryString>
		<field name="name" class="java.lang.String"/>
		<field name="price_unit" class="java.lang.Number">
			<fieldDescription><![CDATA[Unit Price]]></fieldDescription>
		</field>
		<field name="product_unit" class="java.lang.String"/>
		<field name="product_qty" class="java.math.BigDecimal"/>
		<field name="product_uom" class="java.lang.String">
			<fieldDescription><![CDATA[UoM]]></fieldDescription>
		</field>
		<variable name="price_subtotal" class="java.lang.Number">
			<variableExpression><![CDATA[$F{price_unit}*$F{product_qty}]]></variableExpression>
			<initialValueExpression><![CDATA[]]></initialValueExpression>
		</variable>
		<variable name="row_count" class="java.lang.Integer" resetType="None" calculation="Count">
			<variableExpression><![CDATA[$V{COLUMN_COUNT}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="IDS" class="java.lang.Object"/>
	<parameter name="PIDS" class="java.lang.Object" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{IDS}.toList()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
po.id po_id,
ou.id ou_id,
ro.logo_path,
ou_partner.image partner_image,
(SELECT ro.name FROM res_org ro
WHERE ro.operating_unit_id = ou.id) partner_name,
(SELECT ro.address_print_text FROM res_org ro
WHERE ro.operating_unit_id = ou.id) address_text,
regexp_replace(ou_partner.street, '\r|\n', ' ', 'g') as partner_street,
ou_township.name partner_township,
ou_district.name partner_district,
ou_province.name partner_province,
ou_partner.vat partner_vat,
ou_partner.taxbranch partner_taxbranch,
ou_partner.zip partner_zip,
ou_partner.phone phone,
ou_partner.fax fax,
po.name po_number,
po.date_order order_date,
(SELECT rfq.name FROM purchase_order rfq WHERE rfq.id = po.quote_id) rfq,
(SELECT rfq.date_order FROM purchase_order rfq WHERE rfq.id = po.quote_id) rfq_date,
vendor.name vendor_name,
vendor.street vendor_street,
vendor_country.name vendor_country,
vendor.zip vendor_zip,
vendor.phone vendor_phone,
vendor.email vendor_mail,
vendor.fax vendor_fax,
apt.note as payment_term,
CONCAT((COALESCE((SELECT rpt.name FROM res_partner_title rpt WHERE rpt.id = approver.title_id)|| ' ','')),approver.first_name,' ',approver.last_name) approve_by,
po.date_doc_approve approve_date,
CONCAT((COALESCE((SELECT rpt.name FROM res_partner_title rpt WHERE rpt.id = verify.title_id)|| ' ','')),verify.first_name,' ',verify.last_name) verify_by,
po.date_verify verify_date,
po.fine_rate fine_rate,
po.fine_condition fine_condition,
po.fine_num_days fine_days,
po.date_fine fine_date,
po.notes notes,
approve_position.name approve_position,
po.partner_ref supplier_ref,
CONCAT((COALESCE((SELECT rpt.name FROM res_partner_title rpt WHERE rpt.id = validater.title_id)|| ' ','')),validater.first_name,' ',validater.last_name) buyer,
(SELECT rp.email FROM res_partner rp WHERE rp.id = validator.partner_id) buyer_mail,
(SELECT rp.phone FROM res_partner rp WHERE rp.id = validator.partner_id) buyer_phone,
taxbranch.name tax_branch_name,
taxbranch.street tax_branch_street,
taxbranch.street2 tax_branch_street2,
taxbranch.city tax_branch_city,
taxbranch.zip tax_branch_zip,
taxbranch.phone tax_branch_phone,
taxbranch.fax tax_branch_fax,
vendor_township.name vendor_township,
vendor_district.name vendor_district,
vendor_province.name vendor_province,
taxbranch.taxid taxbranch_taxid,
taxbranch.code taxbranch_code,
po.amount_untaxed sub_total,
po.amount_tax vat,
po.amount_total grand_total,
po.amount_total_text_en amount_total_EN,
po.delivery_address,
si.code incoterm,
cur.name as currency,
po.incoterm_text
FROM purchase_order po
LEFT JOIN operating_unit ou
ON ou.id = po.operating_unit_id
LEFT JOIN res_org ro
ON ro.operating_id = ou.id
LEFT JOIN res_partner ou_partner
ON ou.partner_id = ou_partner.id
LEFT JOIN res_country_township ou_township
ON ou_township.id = ou_partner.township_id
LEFT JOIN res_country_district ou_district
ON ou_district.id = ou_partner.district_id
LEFT JOIN res_country_province ou_province
ON ou_province.id = ou_partner.province_id
LEFT JOIN res_partner vendor
ON po.partner_id = vendor.id
LEFT JOIN res_country vendor_country
ON vendor_country.id = vendor.country_id
LEFT JOIN res_country_township vendor_township
ON vendor_township.id = vendor.township_id
LEFT JOIN res_country_district vendor_district
ON vendor_district.id = vendor.district_id
LEFT JOIN res_country_province vendor_province
ON vendor_province.id = vendor.province_id
LEFT JOIN account_payment_term apt
ON apt.id = po.payment_term_id
LEFT JOIN res_users approve_user
ON approve_user.id = po.doc_approve_uid
LEFT JOIN hr_employee approver
ON approver.employee_code = approve_user.login
LEFT JOIN res_users verify_user
ON verify_user.id = po.verify_uid
LEFT JOIN hr_employee verify
ON verify.employee_code = verify_user.login
LEFT JOIN hr_employee he
ON he.employee_code = approve_user.login
LEFT JOIN hr_position approve_position
ON approve_position.id = he.position_id
LEFT JOIN res_users validator
ON validator.id = po.validator
LEFT JOIN hr_employee validater
ON validater.employee_code = validator.login
LEFT JOIN res_taxbranch taxbranch
ON taxbranch.id = po.taxbranch_id
LEFT JOIN stock_incoterms si
ON si.id = po.incoterm_id
LEFT JOIN res_currency cur
ON cur.id = po.currency_id
WHERE $X{IN,po.id,PIDS}]]>
	</queryString>
