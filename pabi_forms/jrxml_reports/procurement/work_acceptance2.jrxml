	<subDataset name="po_committee">
		<parameter name="ids" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
poc.name,
poc.position
FROM purchase_order_committee poc
LEFT JOIN purchase_committee_type pct
ON pct.id = poc.committee_type_id
WHERE poc.order_id = $P{ids}
AND pct.name like '%พัสดุ%'
ORDER BY poc.id]]>
		</queryString>
		<field name="name" class="java.lang.String"/>
		<field name="position" class="java.lang.String">
			<fieldDescription><![CDATA[Position]]></fieldDescription>
		</field>
	</subDataset>
	<subDataset name="work_acceptance_line">
		<parameter name="ids" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
row_number() over (order by pwal.id) as pwa_count,
trim(pwal.name) acceptance_line_name,
pwal.to_receive_qty acceptance_line_qty,
CONCAT(pwal.to_receive_qty::integer,' ',(SELECT value FROM ir_translation it WHERE
res_id = pu.id AND it.name LIKE 'product.uom,name' limit 1)) acceptance_line_uom,
pwal.price_unit acceptance_line_unit_price,
pwal.price_subtotal acceptance_line_price_subtotal,
pwa.amount_untaxed as amount_untaxed,
pwa.amount_tax as amount_tax,
pwa.amount_total as amount_total
FROM purchase_work_acceptance_line pwal
LEFT JOIN purchase_work_acceptance pwa
ON pwa.id = pwal.acceptance_id
LEFT JOIN product_product pp
ON pp.id = pwal.product_id
LEFT JOIN product_uom pu
ON pu.id = pwal.product_uom
LEFT JOIN purchase_order_line pol
ON pol.id = pwal.line_id
WHERE pwal.acceptance_id = $P{ids}
ORDER BY pwal.id]]>
		</queryString>
		<field name="pwa_count" class="java.lang.Long"/>
		<field name="acceptance_line_name" class="java.lang.String"/>
		<field name="acceptance_line_qty" class="java.math.BigDecimal"/>
		<field name="acceptance_line_uom" class="java.lang.String"/>
		<field name="acceptance_line_unit_price" class="java.lang.Double"/>
		<field name="acceptance_line_price_subtotal" class="java.lang.Double"/>
		<field name="amount_untaxed" class="java.lang.Double">
			<fieldDescription><![CDATA[Subtotal]]></fieldDescription>
		</field>
		<field name="amount_tax" class="java.lang.Double">
			<fieldDescription><![CDATA[Tax]]></fieldDescription>
		</field>
		<field name="amount_total" class="java.lang.Double">
			<fieldDescription><![CDATA[Total]]></fieldDescription>
		</field>
		<variable name="line_count" class="java.lang.Integer" resetType="None" calculation="Count">
			<variableExpression><![CDATA[$V{COLUMN_COUNT}]]></variableExpression>
		</variable>
		<variable name="subtotal" class="java.lang.Number">
			<variableExpression><![CDATA[$F{acceptance_line_qty}*$F{acceptance_line_unit_price}]]></variableExpression>
		</variable>
		<variable name="total" class="java.lang.Number" calculation="Sum">
			<variableExpression><![CDATA[$V{subtotal}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="vat" class="java.lang.Number">
			<variableExpression><![CDATA[$V{total}*0.07]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="grand_total" class="java.lang.Number">
			<variableExpression><![CDATA[$V{total}+$V{vat}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<parameter name="IDS" class="java.lang.Object"/>
	<parameter name="PIDS" class="java.util.List" isForPrompting="false">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA[$P{IDS}.toList()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
pwa.id pwa_id,
rt.name tax_branch_name,
(Case when ou.code = 'สำนักงานกลาง'
then
'สำนักงานพัฒนาวิทยาศาสตร์และเทคโนโลยีแห่งชาติ'
else
ou.code
end) as operation_unit_name,
ou.id ou_id,
pwa.name gr_no,
pwa.create_date create_date,
po.name po_name,
po.date_order + '7 hours'::interval po_date,
pwa.date_accept wa_date,
pwa.supplier_invoice invoice_no,
pwa.date_invoice invoice_date,
pwa.date_contract_start contract_start_date,
pwa.date_scheduled_end contract_end_date,
con.start_date contract_sys_date,
supplier.name supplier_name,
supplier.vat supplier_tax_id,
supplier.taxbranch supplier_taxbranch,
pwa.date_receive receive_date,
pwa.eval_receiving deliver_score,
pwa.eval_quality quality_score,
pwa.eval_service service_score,
po.id po_id,
(pwa.overdue_day) contract,
pwa.is_manual_fine,
pwa.manual_fine,
total_fine_cal fine,
pwa.total_fine total_fine,
pwa.amount_tax,
pwa.amount_untaxed,
pwa.amount_total,
pwa.manual_days,
con.display_code as contract_name,
con.action_date,
case when cur.name = 'THB' then
'จำนวนเงิน(บาท)'
else
CONCAT('จำนวนเงิน(',cur.name,')')
end
as currency,
ro.logo_path
FROM purchase_work_acceptance pwa
LEFT JOIN purchase_order po
ON po.id = pwa.order_id
LEFT JOIN purchase_contract con
ON po.contract_id = con.id
LEFT JOIN res_taxbranch rt
ON po.taxbranch_id = rt.id
LEFT JOIN operating_unit ou
ON ou.id = po.operating_unit_id
LEFT JOIN res_partner rp
ON rp.id = ou.partner_id
LEFT JOIN res_currency cur
ON cur.id = po.currency_id
LEFT JOIN res_partner supplier
ON supplier.id = po.partner_id
LEFT JOIN res_org ro
ON ro.operating_unit_id = ou.id
WHERE $X{IN,pwa.id,PIDS}]]>
	</queryString>
