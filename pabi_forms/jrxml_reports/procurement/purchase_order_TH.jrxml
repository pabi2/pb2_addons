	<queryString>
		<![CDATA[SELECT DISTINCT
po.id po_id,
ou.id ou_id,
ou_partner.image partner_image,
(Case when ou.code = 'สำนักงานกลาง'
then
'สำนักงานพัฒนาวิทยาศาสตร์และเทคโนโลยีแห่งชาติ'
else
ou.code
end) as partner_name,
regexp_replace(ou_partner.street, '\r|\n', ' ', 'g') as partner_street,
regexp_replace(ou_partner.street2, '\r|\n', ' ', 'g') as partner_street2,
ou_township.name partner_township,
ou_district.name partner_district,
ou_province.name partner_province,
ou_partner.vat partner_vat,
ou_partner.taxbranch partner_taxbranch,
ou_partner.zip partner_zip,
ou_partner.phone phone,
ou_partner.fax fax,
po.name po_number,
po.date_order + '7 hours'::interval order_date,
(SELECT rfq.name FROM purchase_order rfq WHERE rfq.id = po.quote_id) rfq,
date_reference rfq_date,
coalesce((SELECT value FROM ir_translation it WHERE
res_id = vendor.title AND it.name LIKE 'res.partner.title,name' limit 1), (SELECT name FROM res_partner_title WHERE id = vendor.title limit 1)) vendor_title,
coalesce((SELECT value FROM ir_translation it WHERE
res_id = vendor.id AND it.name LIKE 'res.partner,name' limit 1), vendor.name) vendor_name,
coalesce((SELECT value FROM ir_translation it WHERE
res_id = vendor.id AND it.name LIKE 'res.partner,street' limit 1), vendor.street) vendor_street,
vendor_country.name vendor_country,
vendor.zip vendor_zip,
vendor.phone vendor_phone,
vendor.fax vendor_fax,
vendor.vat vendor_tax,
(Case when vendor.taxbranch = '00000'
then
'สำนักงานใหญ่'
else
vendor.taxbranch
end) as vendor_branch,
(SELECT value FROM ir_translation it WHERE
res_id = apt.id AND it.name LIKE 'account.payment.term,note' limit 1) payment_term,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = he.title_id AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it WHERE
res_id = he.id AND it.name LIKE 'hr.employee,first_name' limit 1),
' ',
(SELECT value FROM ir_translation it WHERE
res_id = he.id AND it.name LIKE 'hr.employee,last_name' limit 1)
) approve_by,
po.date_doc_approve approve_date,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = verify_hr.title_id AND it.name LIKE 'res.partner.title,name' limit 1) || ' ', ''),
(SELECT value FROM ir_translation it WHERE
res_id = verify_hr.id AND it.name LIKE 'hr.employee,first_name' limit 1),
' ',
(SELECT value FROM ir_translation it WHERE
res_id = verify_hr.id AND it.name LIKE 'hr.employee,last_name' limit 1)
) verify_by,
po.date_verify verify_date,
po.fine_rate fine_rate,
po.fine_condition fine_condition,
po.fine_num_days fine_days,
po.fine_num_months fine_month,
po.date_fine fine_date,
po.notes notes,
(SELECT value
FROM ir_translation it
WHERE it.res_id = he.position_id AND it.name = 'hr.position,name' limit 1) approve_position,
po.partner_ref supplier_ref,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = validator_hr.title_id AND it.name LIKE 'res.partner.title,name' limit 1) || ' ', ''),
(SELECT value FROM ir_translation it WHERE
res_id = validator_hr.id AND it.name LIKE 'hr.employee,first_name' limit 1),
' ',
(SELECT value FROM ir_translation it WHERE
res_id = validator_hr.id AND it.name LIKE 'hr.employee,last_name' limit 1)
) buyer,
validater.phone buyer_phone,
validater.email buyer_email,
(SELECT value
FROM ir_translation it
WHERE it.res_id = taxbranch.id AND it.name = 'res.taxbranch,name' limit 1) as tax_branch_name,
(SELECT value
FROM ir_translation it
WHERE it.res_id = ro.id AND it.name = 'res.org,address_print_text' limit 1) as tax_branch_org,
(SELECT value
FROM ir_translation it
WHERE it.res_id = taxbranch.id AND it.name = 'res.taxbranch,street' limit 1) as tax_branch_street,
(SELECT value
FROM ir_translation it
WHERE it.res_id = taxbranch.id AND it.name = 'res.taxbranch,street2' limit 1) as tax_branch_street2,
(SELECT value
FROM ir_translation it
WHERE it.res_id = taxbranch.id AND it.name = 'res.taxbranch,city' limit 1) as tax_branch_city,
taxbranch.zip as tax_branch_zip,
taxbranch.phone as tax_branch_phone,
taxbranch.fax as tax_branch_fax,
vendor_township.name vendor_township,
vendor_district.name vendor_district,
vendor_province.name vendor_province,
taxbranch.taxid as taxbranch_taxid,
taxbranch.code as taxbranch_code,
po.amount_untaxed sub_total,
po.amount_tax vat,
po.amount_total grand_total,
po.amount_total_text_TH amount_total_TH,
po.delivery_address,
(SELECT CONCAT((amount*100)::integer::char,'%')
FROM purchase_order_taxe pot
LEFT JOIN account_tax at
ON at.id = pot.tax_id
WHERE ord_id = po.id limit 1) percentvat,
row_number() over (order by pol.id) as pol_id,
pol.name,
pol.price_unit,
SUM(pol.product_qty) product_qty,
(SELECT value FROM ir_translation it WHERE
res_id = pu.id AND it.name LIKE 'product.uom,name' limit 1) as product_uom
FROM purchase_order po
 LEFT JOIN purchase_order_line pol
 ON pol.order_id = po.id
LEFT JOIN product_uom pu
ON pu.id = pol.product_uom
 LEFT JOIN operating_unit ou
 ON ou.id = po.operating_unit_id
 LEFT JOIN res_partner ou_partner
 ON ou.partner_id = ou_partner.id
 LEFT JOIN res_country_township ou_township
 ON ou_township.id = ou_partner.township_id
 LEFT JOIN res_country_district ou_district
 ON ou_district.id = ou_partner.district_id
 LEFT JOIN res_country_province ou_province
 ON ou_province.id = ou_partner.province_id
 LEFT JOIN res_partner vendor
 ON po.partner_id = vendor.id
 LEFT JOIN res_country vendor_country
 ON vendor_country.id = vendor.country_id
 LEFT JOIN res_country_township vendor_township
 ON vendor_township.id = vendor.township_id
 LEFT JOIN res_country_district vendor_district
 ON vendor_district.id = vendor.district_id
 LEFT JOIN res_country_province vendor_province
 ON vendor_province.id = vendor.province_id
 LEFT JOIN account_payment_term apt
 ON apt.id = po.payment_term_id
 LEFT JOIN res_users approve_user
 ON approve_user.id = po.doc_approve_uid
 LEFT JOIN res_partner approver
 ON approver.id = approve_user.partner_id
 LEFT JOIN res_users verify_user
 ON verify_user.id = po.verify_uid
 LEFT JOIN res_partner verify
 ON verify.id = verify_user.partner_id
 LEFT JOIN hr_employee verify_hr
 ON verify_user.login = verify_hr.employee_code
 LEFT JOIN hr_employee he
 ON he.employee_code = approve_user.login
 LEFT JOIN hr_position approve_position
 ON approve_position.id = he.position_id
 LEFT JOIN res_users validator
 ON validator.id = po.validator
 LEFT JOIN hr_employee validator_hr
 ON validator_hr.employee_code = validator.login
 LEFT JOIN res_partner validater
 ON validater.id = validator.partner_id
 LEFT JOIN res_taxbranch taxbranch
 ON taxbranch.id = po.taxbranch_id
 LEFT JOIN res_org ro
 ON ro.operating_unit_id = ou.id
WHERE $X{IN, po.id, PIDS}
 GROUP BY po.id,pol.id,ou.id,ou_partner.image,ou_partner.street,ou_partner.street2,ou_township.name,ou_district.name,ou_province.name,ou_partner.vat,ou_partner.taxbranch
 ,ou_partner.zip,ou_partner.phone,ou_partner.fax,vendor.title,vendor.id,vendor_country.name,apt.id,he.title_id,he.id,verify_hr.title_id,verify_hr.id,
 validator_hr.title_id,validator_hr.id,validater.phone,validater.email,taxbranch.id,vendor_township.name,vendor_district.name,vendor_province.name,
 pol.name,pol.price_unit,pu.id,ro.id]]>
	</queryString>
