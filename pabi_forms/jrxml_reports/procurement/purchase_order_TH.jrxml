	<subDataset name="po_line">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
row_number() over (order by pol.id) as pol_id,
pol.name,
SUM(pol.product_qty) price_unit,
SUM(pol.price_unit) price_unit,
--(SELECT value
--FROM ir_translation it
--WHERE it.res_id = pu.id AND it.name = 'product.uom,name') product_uom
(SELECT value FROM ir_translation it WHERE
res_id = pu.id AND it.name LIKE 'product.uom,name' limit 1) as product_uom
FROM purchase_order_line pol
LEFT JOIN product_uom pu
ON pu.id = pol.product_uom
WHERE pol.order_id = $P{ids}
GROUP BY pol.id,pu.id]]>
		</queryString>
		<field name="pol_id" class="java.lang.Integer"/>
		<field name="pol_count" class="java.lang.Long"/>
		<field name="name" class="java.lang.String"/>
		<field name="price_unit" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[Unit Price]]></fieldDescription>
		</field>
		<field name="product_qty" class="java.math.BigDecimal"/>
		<field name="product_uom" class="java.lang.String">
			<fieldDescription><![CDATA[UoM]]></fieldDescription>
		</field>
		<variable name="price_subtotal" class="java.lang.Number">
			<variableExpression><![CDATA[$F{price_unit}*$F{product_qty}]]></variableExpression>
			<initialValueExpression><![CDATA[]]></initialValueExpression>
