<openerp>
    <data noupdate="1">
        <!--
          * Purchase Requisition
          * Work Acceptance
          * Approval Report
          * Stock Request
          * Stock Transfer
          * Stock Borrow
          * Payment Export
          * Account Interface
          * POS Order
        -->
        <record id="doctype_purchase_requisition" model="res.doctype" >
            <field name="name">Purchase Requisition</field>
            <field name="sequence_id" ref="doctype_purchase_requisition_seq"/>
            <field name="refer_type">purchase_requisition</field>
        </record>
        <record id="doctype_work_acceptance" model="res.doctype" >
            <field name="name">Work Acceptance</field>
            <field name="sequence_id" ref="doctype_work_acceptance_seq"/>
            <field name="refer_type">work_acceptance</field>
        </record>
        <record id="doctype_approval_report" model="res.doctype" >
            <field name="name">Approval Report</field>
            <field name="sequence_id" ref="doctype_approval_report_seq"/>
            <field name="refer_type">approval_report</field>
        </record>
        <record id="doctype_stock_request" model="res.doctype" >
            <field name="name">Stock Request</field>
            <field name="sequence_id" ref="doctype_stock_request_seq"/>
            <field name="refer_type">stock_request</field>
        </record>
        <record id="doctype_stock_transfer" model="res.doctype" >
            <field name="name">Stock Transfer</field>
            <field name="sequence_id" ref="doctype_stock_transfer_seq"/>
            <field name="refer_type">stock_transfer</field>
        </record>
        <record id="doctype_stock_borrow" model="res.doctype" >
            <field name="name">Stock Borrow</field>
            <field name="sequence_id" ref="doctype_stock_borrow_seq"/>
            <field name="refer_type">stock_borrow</field>
        </record>
        <record id="doctype_payment_export" model="res.doctype" >
            <field name="name">Payment Export</field>
            <field name="sequence_id" ref="doctype_payment_export_seq"/>
            <field name="refer_type">payment_export</field>
        </record>
        <record id="doctype_interface_account" model="res.doctype" >
            <field name="name">Interface Account</field>
            <field name="sequence_id" ref="doctype_interface_account_seq"/>
            <field name="refer_type">interface_account</field>
        </record>

        <!--Additional Reversal Data-->
        <record id="doctype_interface_account" model="res.doctype" >
            <field name="with_reversal" >True</field>
            <field name="reversal_sequence_id" ref="l10n_th_doctype_reversal.doctype_cancel_with_reversal_seq"/>
        </record>

        <!-- Advance Dunning -->
        <record id="doctype_advance_dunning_letter" model="res.doctype" >
            <field name="name">Advance Dunning Letter</field>
            <field name="sequence_id" ref="doctype_advance_dunning_letter_seq"/>
            <field name="refer_type">advance_dunning_letter</field>
        </record>
        <!-- Asset Management -->
        <record id="doctype_asset_transfer" model="res.doctype" >
            <field name="name">Asset Transfer</field>
            <field name="sequence_id" ref="doctype_asset_transfer_seq"/>
            <field name="refer_type">asset_transfer</field>
        </record>
        <record id="doctype_asset_changeowner" model="res.doctype" >
            <field name="name">Asset Change Owner</field>
            <field name="sequence_id" ref="doctype_asset_changeowner_seq"/>
            <field name="refer_type">asset_changeowner</field>
        </record>
        <record id="doctype_asset_request" model="res.doctype" >
            <field name="name">Asset Request</field>
            <field name="sequence_id" ref="doctype_asset_request_seq"/>
            <field name="refer_type">asset_request</field>
        </record>
        <record id="doctype_asset_removal" model="res.doctype" >
            <field name="name">Asset Removal</field>
            <field name="sequence_id" ref="doctype_asset_removal_seq"/>
            <field name="refer_type">asset_removal</field>
        </record>
        <record id="doctype_asset_adjust" model="res.doctype" >
            <field name="name">Asset Adjustment</field>
            <field name="sequence_id" ref="doctype_asset_adjust_seq"/>
            <field name="refer_type">asset_adjust</field>
            <field name="with_reversal" >True</field>
            <field name="reversal_sequence_id" ref="doctype_cancel_with_reversal_seq"/>
        </record>
        <!-- Misc -->
        <record id="doctype_loan_installment" model="res.doctype" >
            <field name="name">Loan Installment</field>
            <field name="sequence_id" ref="doctype_loan_installment_seq"/>
            <field name="refer_type">loan_installment</field>
        </record>
        <record id="doctype_bank_reconcile" model="res.doctype" >
            <field name="name">Bank Statement Reconcile</field>
            <field name="sequence_id" ref="doctype_bank_reconcile_seq"/>
            <field name="refer_type">bank_reconcile</field>
        </record>
        <record id="doctype_purchase_billing" model="res.doctype" >
            <field name="name">Purchase Billing</field>
            <field name="sequence_id" ref="doctype_purchase_billing_seq"/>
            <field name="refer_type">purchase_billing</field>
        </record>
        <record id="doctype_pos_order" model="res.doctype" >
            <field name="name">POS Order</field>
            <field name="sequence_id" ref="doctype_pos_order_seq"/>
            <field name="refer_type">pos_order</field>
        </record>

    </data>
</openerp>
