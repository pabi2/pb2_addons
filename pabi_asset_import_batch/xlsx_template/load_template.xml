<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- This function is called after all ir.attachment is loaded
            to load the file content to relevant template_ids -->
        <function model="ir.attachment" name="load_xlsx_template">
            <value eval="[ref('import_asset_batch_template'),
            				ref('import_asset_batch_template_download'), ]"/>
            <value>pabi_asset_import_batch/xlsx_template</value>
        </function>
    </data>
</openerp>
